{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "timezones",
  "title": "Timezones",
  "description": "Nifty timezone tool for Raycast users",
  "icon": "command-icon.png",
  "author": "Gabriel",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search and convert timezones",
      "subtitle": "Search individual timezones or pairs",
      "description": "Display timezone time or converts based on input",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.29.1",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}